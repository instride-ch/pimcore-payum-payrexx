{
  "name": "instride/pimcore-payum-payrexx",
  "type": "pimcore-bundle",
  "description": "CoreShop - Payrexx Bundle",
  "keywords": [
    "coreshop",
    "payment",
    "payrexx",
    "ecommerce"
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "instride AG",
      "email": "connect@instride.ch",
      "homepage": "https://instride.ch",
      "role": "Digital Agency"
    }
  ],
  "require": {
    "php": ">=7.4",
    "php-http/guzzle6-adapter": "^2.0",
    "instride/payum-payrexx": "^1.0"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Instride\\Bundle\\PimcorePayumPayrexxBundle\\": "src/PimcorePayumPayrexxBundle"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    },
    "pimcore": {
      "bundles": [
        "Instride\\Bundle\\PimcorePayumPayrexxBundle\\PimcorePayumPayrexxBundle"
